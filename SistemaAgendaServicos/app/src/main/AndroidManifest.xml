<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sistemaagendaservicos">

    <uses-permission
        android:name="android.permission.GET_ACCOUNTS"
        android:maxSdkVersion="22" />

    <uses-permission android:name="android.permission.READ_CONTACTS\"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconeagendamentocliente"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iconeagendamentocliente"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.AgendamentoServicoActivity"></activity>
        <activity android:name=".activity.HorariosActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>